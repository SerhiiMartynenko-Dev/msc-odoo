<?xml version="1.0" encoding="UTF-8" ?>
<odoo>



    <record id="view_wizard_print_product_label_form" model="ir.ui.view">
        <field name="name">msc.wizard.print_product_label.form</field>
        <field name="model">msc.wizard.print_product_label</field>
        <field name="arch" type="xml">
            <form create="0" edit="0" duplicate="0" delete="0">
                <sheet>
                    <field name="id" invisible="1" />

                    <group>
                        <field name="mode" widget="radio" />
                    </group>
                </sheet>
                <footer>
                    <button type="object" name="action_print"
                            string="Print" icon="fa-print" class="btn-success" />
                    <button special="cancel" string="Cancel" />
                </footer>
            </form>
        </field>
    </record>



</odoo>
