<?xml version="1.0" encoding="utf-8"?>
<odoo>



    <record id="view_picking_form" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit.msc</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form" />
        <field name="priority" eval="999" />
        <field name="arch" type="xml">
            <field name="move_ids_without_package" position="after">
                <group name="note_group" col="6" class="mt-2 mt-md-0">
                    <group colspan="4">
                    </group>
                    <group class="oe_subtotal_footer oe_right" colspan="2" name="total">
                        <field name="total_demand" />
                        <field name="total_done" />
                    </group>
                    <div class="oe_clear"/>
                </group>
            </field>
        </field>
    </record>


</odoo>
